defaults
    log     global
    mode    http
    option  httplog

frontend web-servers
    bind *:80
    default_backend app-servers

backend app-servers
    balance roundrobin
    server 330106-web-01 54.144.149.55:80 check
    server 330106-web-02 35.175.135.127:80 check

#--jumaongoro.tech params----begining
frontend  jumaongoro.tech-https-frontend
        bind *:80
        bind *:443 ssl crt /etc/ssl/certs/ongoro.pem
        mode http
        default_backend jumaongoro.tech-backend


backend jumaongoro.tech-backend
        balance roundrobin
        server 330106-web-01 54.144.149.55:80 check
        server 330106-web-02 35.175.135.127:80 check
# end of parameters
